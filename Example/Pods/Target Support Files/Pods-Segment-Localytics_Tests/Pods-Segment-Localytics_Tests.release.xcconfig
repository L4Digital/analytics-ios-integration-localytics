FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Localytics/Localytics-iOS-4.0.0" "$(PLATFORM_DIR)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Analytics" "${PODS_ROOT}/Headers/Public/Expecta" "${PODS_ROOT}/Headers/Public/Localytics" "${PODS_ROOT}/Headers/Public/Segment-Localytics" "${PODS_ROOT}/Headers/Public/Specta"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/Analytics" "$PODS_CONFIGURATION_BUILD_DIR/Expecta" "$PODS_CONFIGURATION_BUILD_DIR/Segment-Localytics" "$PODS_CONFIGURATION_BUILD_DIR/Specta"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Analytics" -isystem "${PODS_ROOT}/Headers/Public/Expecta" -isystem "${PODS_ROOT}/Headers/Public/Localytics" -isystem "${PODS_ROOT}/Headers/Public/Segment-Localytics" -isystem "${PODS_ROOT}/Headers/Public/Specta"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Analytics" -l"Expecta" -l"Segment-Localytics" -l"Specta" -l"sqlite3" -l"z" -framework "AdSupport" -framework "CoreLocation" -framework "Foundation" -framework "Localytics" -framework "SystemConfiguration" -framework "XCTest"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
