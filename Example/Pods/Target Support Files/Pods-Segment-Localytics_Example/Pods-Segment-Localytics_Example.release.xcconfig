FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Localytics/Localytics-iOS-4.0.0"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Localytics"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/Analytics.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Segment_Localytics.framework/Headers" -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Localytics"
OTHER_LDFLAGS = $(inherited) -l"sqlite3" -l"z" -framework "AdSupport" -framework "Analytics" -framework "CoreLocation" -framework "Localytics" -framework "Segment_Localytics" -framework "SystemConfiguration"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Segment-Localytics_Example
PODS_ROOT = ${SRCROOT}/Pods